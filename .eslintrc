{
    "extends": "kellyirc",
    "parser": "babel-eslint",
    "plugins": [],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "ecmaFeatures": {
    },
    "rules": {
        "constructor-super": 2,
        "no-this-before-super": 2,
        "no-var": 2,
        "object-shorthand": [2, "always"],
        "prefer-const": 2,
        "object-curly-spacing": [2, "always"],
        "no-unused-vars": 0,
        "no-console": 0,
        "curly": 0,
        "no-underscore-dangle": 0,
        "indent": [2, "tab"],
        "no-constant-condition": 0
    }
}
